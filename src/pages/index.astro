---
import Layout from "../layouts/base.astro";
import { Translate } from "../index.js";
import { SetDefaultLanguage } from "../index.js";
import { LanguageSwitcher } from "../index.js";
import { getLocalizationMap } from "../utils/localizationHelpers.js";

var localizationMap = getLocalizationMap();
---

<Layout>
	<SetDefaultLanguage />
	<LanguageSwitcher />
	
	<br />
	<Translate map={localizationMap} key="test-key" />
	<br />
	<Translate map={localizationMap} key="Test" />
	<br />
	<Translate key="Test" />
	<br />
	<Translate key="test-key" />
	<br />
	<Translate />
</Layout>

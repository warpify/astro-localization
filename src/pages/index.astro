---
import Layout from "../layouts/base.astro";
import { Translate } from "../index.js";
import { SetDefaultLanguage } from "../index.js";
import { LanguageSwitcher } from "../index.js";

import it from "../data/localization/it.json";
import en from "../data/localization/en.json";

var lamp = { it: it, en: en };
---

<Layout>
	<SetDefaultLanguage />
	<LanguageSwitcher />

	<br />
	<Translate lmap={lamp} key="test-key" />
	<br />
	<Translate lmap={lamp} key="Test" />
	<br />
	<Translate key="Test" />
	<br />
	<Translate key="test-key" />
	<br />
	<Translate />
</Layout>
